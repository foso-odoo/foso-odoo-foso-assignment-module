<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="custom_res_config_settings_view_form" model="ir.ui.view">
            <field name="name">custom.res.config.settings.view.form.inherit.stock</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="30"/>
            <field name="inherit_id" ref="stock.res_config_settings_view_form" />
            <field name="arch" type="xml">
                <!-- <xpath expr="//app[@name='stock']/block[@name='shipping_setting_container']/setting[@name='stock_sms']" position="before"> -->
                     <xpath expr="//block[@name='shipping_setting_container']/setting[@id='stock_sms']" position="before">
                            <setting id="stock_transport_module" string="Stock Transport" help="Help to install stock module">
                                <field name="module_stock_transport"/>
                            </setting>
                        
                    </xpath>
            </field>
        </record>
    </data>
</odoo>
